{
  "name": "customergauge/bref",
  "description": "AWS Lambda with Bref Adapter for Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "bref", "aws", "lambda"],
  "authors": [
    {
      "name": "Marco Deleu",
      "email": "deleugyn@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "bref/bref": "^0.5.15",
    "illuminate/contracts": ">=5.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "CustomerGauge\\Bref\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomerGauge\\Bref\\LaravelBrefServiceProvider"
      ]
    }
  }
}
